#include<fstream>
#include<iostream>
#include <string>
using namespace std;
int main()
{
    ofstream fout("Файлик");
     cout << "Введите первое число:";
    int a, b;
    cin >> a; 
    cout << "Введите второе число, но большее, чем первое:";
    cin >> b;
    if (b>a);
	else
		cout << "Всё фигня, начинаем заново(второе число > первого)";
		if (b>a);
		else
		return 0;
    for (int i = a; i <= b ; i++)
    {
        fout << i * 3 << endl;
    }
    fout.close();

string line;
int itog;
ifstream file ("Файлик");
if (file.is_open()) {
itog = 0;
cout << "Полученные значения: \n";
while ( getline (file, line) ) {
cout << ++itog << ". ";
cout << line << '\n';
}
file.close();
}
}
