#include <iostream>
using namespace std;
int main()
{
	char nums[10][20] = { "Ноль","Один", "Два", "Три", "Четыре", "Пять", "Шесть", "Семь", "Восемь", "Девять" };
	cout << "Введите число: ";
	int n;
	cin >> n;
	if (n >= 0 && n <= 9)
		cout << nums[n] << "\n";
	else
		cout << "Не знаю такого числа, умею считать лишь до девяти\n";
	return 0;
}
