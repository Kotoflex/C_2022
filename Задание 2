#include <iostream>
#include <fstream>
using namespace std;

int main()
{
    int a, b, c;
    cout << "Введите два числа (второе больше первого): ";
    cin >> a >> b;
    int raz = b - a;
    ofstream file("Output");
    for (a; a < b+1; a++){
        file << a * 3 << "\n";
    }
    file.close();
    
    
    cout << "Результат: \n";
    int cif;
    int nom = 1;
    ifstream file1("Output");
    while(nom <= raz + 1){
        file1 >> cif;
        cout << nom++ << ". " << cif << "\n";
    }
    file1.close();
    return 0;
}
