
#include <iostream>
using namespace std;
 
int main() {
    int i1, i2; // Целое
    float f1, f2; // Точка плавает
bool b = true; // правда или ложь 
double d1, d2; // Точка плавает точнее
 cout << "Умножаем целые числа:";
    cin >> i1 >> i2;
    int res = i1 * i2; // Умножаем целое
    cout << "Я сделяль целое " << res << endl; 
     cout << "Плюсуем плавающую точку:";
  cin >> f1 >> f2;
     float F = f1 + f2; // + точка
   cout << "Я сделяль плавающую точку: " << F << endl; 
      cout << "Минусуем точную точечку:";
      cin >> d1 >> d2;
     double D = d1 - d2; // - точная точка
   cout << "Даже double получился: " << D << endl;
    cout << "А что можно придумать с этим? " << b << endl;
    return 0; // На этом всё
}
